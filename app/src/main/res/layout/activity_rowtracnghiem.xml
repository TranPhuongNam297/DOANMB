<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_Dap_An_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_Cau_HoiTN"
        app:layout_constraintTop_toBottomOf="@+id/tv_Cau_HoiTN" />

    <TextView
        android:id="@+id/tv_Dap_An_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_Dap_An_1"
        app:layout_constraintTop_toBottomOf="@+id/tv_Dap_An_1" />

    <TextView
        android:id="@+id/tv_Cau_HoiTN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_Dap_An_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_Dap_An_2"
        app:layout_constraintTop_toBottomOf="@+id/tv_Dap_An_2" />

    <TextView
        android:id="@+id/tv_Dap_An_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_Dap_An_3"
        app:layout_constraintTop_toBottomOf="@+id/tv_Dap_An_3" />

    <TextView
        android:id="@+id/tv_Dap_An_Dung_TN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_Dap_An_4"
        app:layout_constraintTop_toBottomOf="@+id/tv_Dap_An_4" />

    <TextView
        android:id="@+id/tv_Muc_Do_TN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/tv_Dap_An_Dung_TN"
        app:layout_constraintTop_toBottomOf="@+id/tv_Dap_An_Dung_TN" />
</androidx.constraintlayout.widget.ConstraintLayout>